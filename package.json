{
  "name": "better-outline",
  "displayName": "Better Outline",
  "description": "Extend and customize the VS Code outline view",
  "version": "0.0.1",
  "publisher": "adamerose",
  "repository": {
    "type": "git",
    "url": "git://github.com/adamerose/better-outline.git"
  },
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "betterOutline.refreshTree",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "betterOutline.addDivider",
        "title": "Add Divider"
      },
      {
        "command": "betterOutline.addHeader",
        "title": "Add Header"
      }
    ],
    "keybindings": [
      {
        "command": "betterOutline.addDivider",
        "key": "alt+d",
        "mac": "alt+d",
        "when": "editorTextFocus"
      },
      {
        "command": "betterOutline.addHeader",
        "key": "alt+shift+d",
        "mac": "alt+shift+d",
        "when": "editorTextFocus"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "betterOutline",
          "name": "Better Outline",
          "contextualTitle": "Better Outline"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "betterOutline",
        "contents": "No outline items found."
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "betterOutline.refreshTree",
          "when": "view == betterOutline",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "Better Outline"
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.46.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.30.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  }
}